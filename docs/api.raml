#%RAML 1.0
title: Teamunite
version: v1
baseUri: https://api.teamunite.ru/{version}
mediaType: application/json
protocols: HTTPS

/announcements:
  description: Запросы для работы с объявлениями
  get:
    description: Получение всех объявлений
    responses:
      200:
        body:
          properties:
            _id:
              type: ObjectId
              description: ID объявления
            title:
              type: String
              description: Заголовок объявления
            description:
              type: String
              description: Описание объявления
            author:
              type: Object
              description: Автор объявления
            createdAt:
              type: Date
              description: Дата создания объявления
          example:
            {
              "_id": "5f8c6757217cb338ebeb2b79",
              "title": "Поступают вопрос от иностранцев, можно ли уезжать домой. Особенно если есть пересдача.",
              "description": "Поступают вопрос от иностранцев, можно ли уезжать домой. Особенно если есть пересдача. Ответ такой: если вы хотите, вы можете уехать. Пересдачи для вас назначат после официального открытия границ.",
              "author": {
                  "_id": "5f8c63b9041a2dd79435c6eb",
                  "name": "Марина",
                  "surname": "Даньшина",
                  "status": "curator"
              },
              "createdAt": "2020-10-18T16:03:35.708Z"
            }
  post:
    description: Добавление объявления
    body: 
      properties:
        title:
          type: String
          description: Заголовок объявления
        description:
          type: String
          description: Описание объявления
        author:
          type: ObjectId
          description: Автор объявления
      example:
        {
          "title": "Поступают вопрос от иностранцев, можно ли уезжать домой. Особенно если есть пересдача.",
          "description": "Поступают вопрос от иностранцев, можно ли уезжать домой. Особенно если есть пересдача. Ответ такой: если вы хотите, вы можете уехать. Пересдачи для вас назначат после официального открытия границ.",
          "author": "5f8c63b9041a2dd79435c6eb"
        }
    responses:
      201:
        body:
          properties:
            _id:
              type: ObjectId
              description: ID объявления
            title:
              type: String
              description: Заголовок объявления
            description:
              type: String
              description: Описание объявления
            author:
              type: ObjectId
              description: Автор объявления
            createdAt:
              type: Date
              description: Дата создания объявления
          example:
            {
              "_id": "5f8c6757217cb338ebeb2b79",
              "title": "Поступают вопрос от иностранцев, можно ли уезжать домой. Особенно если есть пересдача.",
              "description": "Поступают вопрос от иностранцев, можно ли уезжать домой. Особенно если есть пересдача. Ответ такой: если вы хотите, вы можете уехать. Пересдачи для вас назначат после официального открытия границ.",
              "author": "5f8c63b9041a2dd79435c6eb",
              "createdAt": "2020-10-18T16:03:35.708Z"
            }
      400:
        body:
          properties:
            message:
              type: String
              description: Ошибка при добавлении
          example:
            {
              "message": "Укажите заголовок объявления"
            }
  /{id}:
    description: Запросы для работы с объявлением
    uriParameters:
      id:
        description: ID объявления
        type: ObjectId
    get:
      description: Получение объявления
      responses:
        200:
          body:
            properties:
              _id:
                type: ObjectId
                description: ID объявления
              title:
                type: String
                description: Заголовок объявления
              description:
                type: String
                description: Описание объявления
              author:
                type: Object
                description: Автор объявления
              createdAt:
                type: Date
                description: Дата создания объявления
            example:
              {
                "_id": "5f8c6757217cb338ebeb2b79",
                "title": "Поступают вопрос от иностранцев, можно ли уезжать домой. Особенно если есть пересдача.",
                "description": "Поступают вопрос от иностранцев, можно ли уезжать домой. Особенно если есть пересдача. Ответ такой: если вы хотите, вы можете уехать. Пересдачи для вас назначат после официального открытия границ.",
                "author": {
                    "_id": "5f8c63b9041a2dd79435c6eb",
                    "name": "Марина",
                    "surname": "Даньшина",
                    "status": "curator"
                },
                "createdAt": "2020-10-18T16:03:35.708Z"
              }
        404:
          body:
            properties:
              message:
                type: String
                description: Сообщение об ошибке
            example: 
              {
                "message": "Объявление не найдено"
              }
/companies:
  description: Запросы для работы с компаниями
  get:
    description: Получение всех компаний
    responses:
      200:
        body:
          properties:
            _id:
              type: ObjectId
