              type: String
              description: Сфера деятельности компании
            logo:
              type: String
              description: Ссылка на логотип компании
            internship:
              type: Boolean
              description: Наличие стажировок в компании
            projects:
              type: Array
              description: Проекты компании
              required: false
          example:
            [
              {
                "_id": "5f8c700f4260fb3e68365c8d",
                "name": "Тинькофф",
                "description": "Тинькофф — технологическая компания с банковской лицензией. Примерно 70% сотрудников штаб-квартиры — IT-специалисты, которые каждый день создают лучшие цифровые продукты в России",
                "activity": "Онлайн-экосистема, основанная на финансовых и лайфстайл-услугах",
                "logo": "/logos/Тинькофф-eRLhfQXPb.jpg",
                "internship": true,
                "projects": [
                  {
                    "status": "review",
                    "_id": "5f8c709fd18f723e90120944",
